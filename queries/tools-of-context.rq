# tools of a context

PREFIX heat: <https://w3id.org/heat#>
PREFIX dct:	<http://purl.org/dc/terms/> 
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

SELECT *
WHERE {
	?_context_iri a heat:Context .
	?contextTool a heat:Tool ;
		rdfs:label ?contextToolLabel ;
		dct:description ?contextToolDescription ;
		heat:hasContext ?_context_iri .
}