#+ endpoint: https://knowledge.c-innovationhub.com/heat
#+ summary: Get the list of all reference theories 
#+ tags:
#+ - ReferenceTheories

PREFIX heat: <https://w3id.org/heat#>
PREFIX dct:	<http://purl.org/dc/terms/> 
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

SELECT ?theory ?theoryLabel ?theoryDescription
WHERE {
	?theory a heat:ReferenceTheory ;
		rdfs:label ?theoryLabel ;
		dct:description ?theoryDescription .
}