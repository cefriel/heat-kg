# constructs of a tool

PREFIX heat: <https://w3id.org/heat#>
PREFIX dct:	<http://purl.org/dc/terms/> 
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

SELECT *
WHERE {
	?_tool_iri a heat:Tool ;
		heat:hasConstruct ?construct .
	?construct a heat:Construct ;
		rdfs:label ?constructLabel ;
		dct:description ?constructDescription .
}